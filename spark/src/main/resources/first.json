{
  "id": 999,
  "name": "firstJob",
  "desc": "test",
  "engineType": "spark",
  "processors": [
    {
      "type": "source",
      "subType": "socket",
      "host": "${user.host}",
      "port": 9999,
      "format": ",",
      "outputTableName": "user"
    },

    {
      "type": "transform",
      "subType": "sql",
      "sql": "select id, count(id) from user group by id",
      "inputTableName": "user",
      "outputTableName": "user_count"
    },

    {
      "type": "action",
      "subType": "redis",
      "host": "localhost",
      "key": "userCount",
      "inputTableName": "user_count"
    }

  ],

  "tableDef": {
    "user": "[{\"name\": \"id\", \"type\": \"int\"}, {\"name\": \"name\", \"type\": \"string\"}]",
    "user_count": "[{\"name\": \"id\", \"type\": \"int\"}, {\"name\": \"count\", \"type\": \"long\"}]"
  },

  "params": {
    "spark.master": "local[4]",
    "spark.duration": 5,
    "user.host": "10.45.47.66"
  }

}