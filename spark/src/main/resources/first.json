{
  "name": "firstJob",
  "desc": "test",
  "engineType": "spark",
  "processors": [
    {
      "type": "source",
      "subType": "socket",
      "host": "localhost",
      "port": 9999,
      "format": ",",
      "outputTableName": "user"
    },

    {
      "type": "transform",
      "subType": "sql",
      "sql": "select id, uuid(name) from user",
      "inputTableName": "user",
      "outputTableName": "user_uuid"
    },

    {
      "type": "action",
      "subType": "console",
      "inputTableName": "user_uuid"
    }

  ],

  "tableDef": {
    "user": "[{\"name\": \"id\", \"type\": \"int\"}, {\"name\": \"name\", \"type\": \"string\"}]",
    "user_uuid": "[{\"name\": \"id\", \"type\": \"int\"}, {\"name\": \"name\", \"type\": \"string\"}]"
  },

  "params": {
    "master": "local[4]",
    "duration": 5,
    "checkpoint": "/Users/apple/spark/checkpoint"
  }

}