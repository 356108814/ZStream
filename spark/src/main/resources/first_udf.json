{
  "name": "firstJob",
  "desc": "test",
  "engineType": "spark",
  "processors": [
    {
      "type": "source",
      "subType": "socket",
      "format": ",",
      "host": "localhost",
      "port": "9999",
      "outputTableName": "user",
      "extClass": "com.ztesoft.zstream.DefaultSourceExtProcessor"
    },

    {
      "type": "transform",
      "subType": "sql",
      "sql": "select id, encrypt(id, id) from user",
      "inputTableName": "user",
      "outputTableName": "user_007"
    },

    {
      "type": "action",
      "subType": "console",
      "inputTableName": "user_007"
    }
  ],

  "tableDef": {
    "user": "[{\"name\": \"id\", \"type\": \"int\"}, {\"name\": \"name\", \"type\": \"string\"}]"
  },

  "udf": {
    "encrypt": "com.ztesoft.zstream.udf.TestEncrypt"
  },

  "params": {
    "master": "local[4]",
    "duration": 5,
    "checkpoint": ""
  }

}